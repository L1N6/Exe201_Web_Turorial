@{
    Coursera coursera;
    List<CourseraDetail> courseraDetail = new List<CourseraDetail>();

    if (TempData.ContainsKey("Coursera"))
    {
        coursera = TempData["Coursera"] as Coursera;
        courseraDetail = coursera.CourseraDetails.ToList();
    }
    else
    {
        coursera = null;
    }
}
<div class="flex-row-a">
    <div class="rectangle-4">
        <span class="time">Thời gian học: @coursera.TotalDate Ngày</span>
    </div>
    <span class="khoa-hoc-bao-gom">
        @coursera.Description<br />
    </span>
</div>
<div class="flex-row-ef">
    @foreach (var item in courseraDetail)
    {
        <div class="group-7"></div>
    }
    <div class="line"></div>
</div>
<div class="flex-row">
    @foreach (var item in courseraDetail)
    {
        <span class="tuan">@item.Name</span>
    }
</div>
<div class="rectangle-c">
    <span class="span">
        @coursera.Description <br />
    </span>
</div>
<div class="flex-row-cc">
    <div class="group-10"></div>
    <div class="group-10"></div>
    <div class="group-10"></div>
    <div class="group-10"></div>
    <div class="line-11"></div>
</div>
<div class="flex-row-f">
    <span class="video">Video</span>
    <span class="documents">Documents</span>
    <span class="test">Test</span>
    <span class="quiz">Quiz</span>
</div>
<div class="flex-row-16">
    <a href="@Url.Action("UniversityEnglishCourseMoocWeek","Course",
    new { courseraId = coursera.CourseraId})" class="group-17">
        <span class="bat-dau">Bắt đầu</span>
    </a>
</div>