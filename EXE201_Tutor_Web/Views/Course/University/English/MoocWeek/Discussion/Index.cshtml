@{
    int courseDetailId = 0;

    if (TempData["CourseraDetailId"] != null)
    {
        courseDetailId = int.Parse(TempData["CourseraDetailId"].ToString());
    }

    Mooc mooc = new Mooc();
    List<Mooc> moocs = new List<Mooc>();
    int courseraId = 0;
    string week = "";
    if (TempData.ContainsKey("Mooc"))
    {
        moocs = TempData["ListMooc"] as List<Mooc>;
        mooc = TempData["Mooc"] as Mooc;
        courseraId = mooc.CourseraDetail.CourseraId;
        week = mooc.CourseraDetail.CodeName;
    }
}
<div class="flex-row-cb">
    <div class="line"></div>
    <div class="group-courseraDetail">
        @foreach (var item in moocs)
        {
            var backgroundColor = item.MoocId == mooc.MoocId ? "background-color:#CDD8FF" : "";
            <a href="@Url.Action("UniversityEnglishCourseMoocWeekPractice", "Course", new {moocId = item.MoocId})"
               class="group" style="@backgroundColor">
                <div class="group-4"></div>
                @if (@item.CodeName.Equals("video"))
                {
                    <span class="span-video">Video</span>
                }
                else if (@item.CodeName.Equals("doc"))
                {
                    <span class="span-video">Document</span>
                }
                else if (@item.CodeName.Equals("quiz"))
                {
                    <span class="span-video">Quiz</span>
                }
                else if (@item.CodeName.Equals("ass"))
                {
                    <span class="span-video">Assignment</span>
                }
                else if (@item.CodeName.Equals("discuss"))
                {
                    <span class="span-video">Discuss</span>
                }
            </a>
        }
    </div>
    <div class="line-8"></div>
</div>
<div class="flex-row-a">
    <div class="regroup">
        <a href="@Url.Action("UniversityEnglishCourseMoocWeek", "Course", new {courseraId = courseraId, week = week})" class="vector-b"></a>
        <div class="polygon"></div>
    </div>
    <span class="tuan-1">@mooc.CourseraDetail.Name</span>
    <span class="menu">Menu</span>
    <div class="group-c"></div>
    <div class="rectangle-d">
        <div class="rectangle-e"></div>
        <span class="assignment">@mooc.Name</span>
    </div>
    <div class="polygon-f"></div>
    <div class="menu-icon"></div>
</div>
<div class="flex-row">
    <div class="hoan-thanh-khoa-hoc">
        <span class="thanh-xuat-sac">Thật xuất sắc!<br /><br /></span><span class="completed-course">
            Bạn đã hoàn thành khóa học Tiếng Anh cơ bản<br />Hãy để lại cảm
            nhận của bạn cho chúng tôi nhé
        </span>
    </div>
</div>
<div class="rectangle-10"></div>
<div class="div-group-11">
    <span class="gui-phan-hoi">Gửi phản hồi</span>
</div>
<script>
    // Lấy tất cả các phần tử có lớp là 'group' trong phần tử có lớp là 'group-courseraDetail'
    var groups = document.querySelectorAll('.group-courseraDetail .group');

    // Tính số lượng phần tử 'group'
    var count = groups.length;

    // Lấy phần tử có lớp là 'line-8'
    var line8 = document.querySelector('.line-8');

    // Tính toán chiều cao dựa trên công thức đã cung cấp
    var height = 150 * (count - 1);

    // Gán chiều cao vào phần tử 'line-8' bằng cách sử dụng biến CSS '--rpx'
    line8.style.height = "calc(" + height + " * var(--rpx))";

</script>